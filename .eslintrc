{
  "extends": "airbnb-base",
  "rules":{
    "no-underscore-dangle":["error", { "allow": ["_id"] }],
    "eqeqeq":["error", "smart"],
    "max-classes-per-file": ["error", 5],
    "no-unused-vars": ["error", { "args": "none" }],
    "comma-dangle": ["error", "never"],
    "no-param-reassign": ["error", { "props": false }]
  }
}